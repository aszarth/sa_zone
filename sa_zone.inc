// get main zone
stock GetMainStateFromCityId(cityid) {
    switch(cityid) {
        case LS_STATE_ID, RC_STATE_ID, FC_STATE_ID: {
            return LS_STATE_ID;
        }
        case SF_STATE_ID, W_STATE_ID: {
            return SF_STATE_ID;
        }
        case LV_STATE_ID, BC_STATE_ID, TR_STATE_ID: {
            return LV_STATE_ID;
        }
    }
    return LC_STATE_ID;
}
stock bool:IsPointInArea(Float:x, Float:y, Float:ax, Float:ay, Float:bx, Float:by) {
    return x >= ax && x <= bx && y >= ay && y <= by;
}
stock GetPosCityId(Float:x, Float:y) {
    for (new i = 0; i != sizeof(MainZones); i++) {
        if (IsPointInArea(x, y, MainZones[i][SAZONE_AREA][0], MainZones[i][SAZONE_AREA][1], MainZones[i][SAZONE_AREA][3], MainZones[i][SAZONE_AREA][4]))
        {
            return MainZones[i][SAZONE_ID];
        }
    }
    return LS_STATE_ID;
}





