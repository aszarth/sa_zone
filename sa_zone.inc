          return west_LasVenturasZones[index][SAZONE_ID];
      }
      case SF_STATE_ID: {
          // east [2]
          if (direction_id == 2) return east_SanFierroZones[index][SAZONE_ID];
          // west [1]
          return west_SanFierroZones[index][SAZONE_ID];
      }
      case RC_STATE_ID: {
          return RedCountyZones[index][SAZONE_ID];
      }
      case FC_STATE_ID: {
          return FlintCountyZones[index][SAZONE_ID];
      }
      case W_STATE_ID: {
          return WhetstoneZones[index][SAZONE_ID];
      }
      case TR_STATE_ID: {
          return TierraRobadaZones[index][SAZONE_ID];
      }
      case BC_STATE_ID: {
          return BoneCountyZones[index][SAZONE_ID];
      }
    }
    return 0;
}

stock SequentialSearch_Zone(cityId, direction_id, Float:x, Float:y)
{
    new size = SAZone_ListLength(cityId, direction_id);
    for(new z = 0; z < size; z++) {
        new Float:min_x = SAZone_GetX1(cityId, z, direction_id);
        new Float:min_y = SAZone_GetY1(cityId, z, direction_id);
        new Float:max_x = SAZone_GetX2(cityId, z, direction_id);
        new Float:max_y = SAZone_GetY2(cityId, z, direction_id);
        if (IsPointInArea(x, y, min_x, min_y, max_x, max_y)) {
            return SAZone_GetID(cityId, z, direction_id);
        }
    }
    return 1;
}
stock GetStreetCityDirection(cityid, Float:x) {
    // 0= none, 1, west, 2= east
    new direction_id = 0;
    // states that have a west/east division (LS_STATE_ID 1, LV_STATE_ID 2, SF_STATE_ID 3)
    if(cityid <= SF_STATE_ID) {
        new direction_index = cityid-1;
        new Float:direction_minX = MainZones[direction_index][SAZONE_AREA][0];
        new Float:direction_maxX = MainZones[direction_index][SAZONE_AREA][3];
        // east
        if (x > (direction_minX + direction_maxX) / 2) direction_id = 2;
        // west
        else direction_id = 1;
    }
    return direction_id;
}
stock GetPosStreetId(cityid, Float:x, Float:y) {
    new direction_id = GetStreetCityDirection(cityid, x);
    new zone_id = SequentialSearch_Zone(cityid, direction_id, x, y);
    return zone_id;
}
